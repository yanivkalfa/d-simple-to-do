  "private": true,
  "description": "simple to do",
  "scripts": {
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.dev.js",
    "start": "NODE_ENV=development npm run debug:server",
    "start:test": "NODE_ENV=development npm-run-all --parallel lint:test debug:server",
    "start:ui-dev": "NODE_ENV=development npm-run-all --parallel lint:test debug:app",
    "debug:app": "node --inspect=5858 server/server.js",
    "debug:server": "nodemon --inspect=5858 server/server.js",
    "lint": "cross-env NODE_ENV=development eslint --ext .jsx .js --color --format=node_modules/eslint-formatter-pretty app server",
    "lint:watch": "npm run lint -- --watch",
    "lint:test": "npm-run-all --parallel lint:watch test:watch",
    "test": "NODE_ENV=test npm-run-all test:app test:server",
    "test:app": "mocha tools/test-setup.js \"app/**/*.js\"",
    "test:server": "NODE_ENV=test mocha \"server/**/*.spec*.js\"",
    "test:watch": "npm run test:app -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm-run-all clean-dist lint prepare:html",
    "prepare:html": "node tools/build-html.js",
    "build": "webpack --config webpack.config.prod.js",
    "prebuild:staging": "npm-run-all clean-dist prepare:html",
    "build:staging": "webpack --config webpack.config.staging.js",
    "seed:dev": "md-seed run users boards",
    "seed:admin": "md-seed run admin"
  },
