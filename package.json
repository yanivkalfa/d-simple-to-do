    "test": "NODE_ENV=test npm-run-all test:app test:server",
    "test:app": "mocha tools/test-setup.js \"app/**/*.js\"",
    "test:server": "NODE_ENV=test mocha \"server/**/*.spec*.js\"",
    "test:watch": "npm run test:app -- --watch",
    "flow": "flow",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
